							<path>bin</path>
							<name>OS_Square.dll</name>
						</assembly>
            <assembly>
              <path>bin</path>
              <name>Square.Connect.dll</name>s
            </assembly>
            <assembly>
              <path>bin</path>
              <name>System.ComponentModel.Annotations.dll</name>s
            </assembly>
            <assembly>
              <path>bin</path>
              <name>System.ComponentModel.DataAnnotations.dll</name>s
            </assembly>
            <assembly>
              <path>bin</path>
              <name>RestSharp.dll</name>s
            </assembly>
